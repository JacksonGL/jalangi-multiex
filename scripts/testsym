rm -f jalangi_sym_test_results 2>/dev/null
rm -rf .coverage_data
rm -rf cover_html
rm -rf .coverage_data_old

export USE_NODE_COVER=false
export USE_TIME=false
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
export ANALYSIS_SCRIPT=$DIR/concolic

$ANALYSIS_SCRIPT tests/unit/bool_symbolic 3
$ANALYSIS_SCRIPT tests/unit/or 3
$ANALYSIS_SCRIPT tests/unit/and 3
$ANALYSIS_SCRIPT tests/unit/path_inputs 4
