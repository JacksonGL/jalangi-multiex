$ANALYSIS_SCRIPT-norr tests/unit/bool_symbolic 3
$ANALYSIS_SCRIPT-norr tests/unit/or 3
$ANALYSIS_SCRIPT-norr tests/unit/and 3
$ANALYSIS_SCRIPT-norr tests/unit/path_inputs 4
$ANALYSIS_SCRIPT-norr tests/unit/summary1 3
$ANALYSIS_SCRIPT-norr tests/unit/integer1 5
$ANALYSIS_SCRIPT-norr tests/unit/integer2 2
